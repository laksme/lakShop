<p id="notice"><%= notice %></p>

<h1>Listing Orders</h1>

<table>
  <thead>
    <tr>
      <th>Address</th>
      <th>City</th>
      <th>State</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @orders.each do |order| %>
      <tr>
        <td><%= order.address %></td>
        <td><%= order.city %></td>
        <td><%= order.state %></td>
        <td><%= link_to 'Show', listing_order_path(order, order.id) %></td>
        <td><%= link_to 'Edit', edit_listing_order_path(order, order.id) %></td>
        <%-# code to destroy order -%>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Order', new_listing_order_path %>
